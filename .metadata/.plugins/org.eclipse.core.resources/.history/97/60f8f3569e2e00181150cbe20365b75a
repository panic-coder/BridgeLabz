
public class AscendingMain{
	public static void main(String[] args) {
		Values c[] = { new Values("shubham",3),
					   new Values("deep",1),
					   new Values("raj",2),
					   new Values("vinay",10),
					   new Values("deepak",8)};
								
		for (int i = 0; i < c.length; i++) {
			for (int j = i; j < c.length-1; j++)
				if(c[j].roll>c[j+1].roll)
				{
					Values temp=c[j];
					c[j]=c[j+1];
					c[j+1]=temp;
				}
		}
		System.out.println("sorted by roll");
		for (int i = 0; i < c.length; i++) {
			System.out.println(c[i]);
		}
		
		for (int i = 0; i < c.length; i++) {
			for (int j = i; j < c.length-1; j++)
				if(compare(c[j].name,c[j+1].name)<0)
				{
					Values temp=c[j];
					c[j]=c[j+1];
					c[j+1]=temp;
				}
		}
		System.out.println("\nsorted by name");
		for (int i = 0; i < c.length; i++) {
			System.out.println(c[i]);
		}
		
	}
	public static int compare(String s1,String s2)
	{
		int len=0;
		if(s1.length()>s2.length())
		{
			len=s1.length();
		}
		else {
			len=s2.length();
		}
		//System.out.println(len);
		for (int i = 0; i < len; i++) {
			while(i<len && s1.charAt(i)-s2.charAt(i)==0)
			{
				i++;
			}
			if(s1.length()==i&&s2.length()==i)
				return 0;
			if(s1.length()==i||s2.length()==i) {
				if(s1.length()==i) {
					return 1;
				}
				else 
					return -1;
			}
			return 0;//(s1.charAt(i)-s2.charAt(i));
		}
		return 1;
	}
}
