import java.util.Scanner;

public class Pattern {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter two values");
		int a = s.nextInt();
		int b = s.nextInt();
		int num=1,num3=0,d,e;
		for(d=1;d<=a;d++) {
			for(e=1;e<d;e++) {
			
			}
			num3=num3+e;
			//System.out.println(num3);
		}
		
		int num2[]=new int[num3];
		
		int arrayCount=0;
		for(int i=b;i>0;i--) {
			for(int j=i;j>0;j--) {
				//System.out.print(" ");
			}
			//System.out.println();
			for(int k=b;k>=i;k--) {
				if(num%2!=0) {
					//System.out.print(num+" ");
					num2[arrayCount]=num;
					arrayCount++;
				}
				num=num+2;
			}
			//System.out.println();
		}
		for(int l=0;l<num2.length;l++)
			System.out.println(num2[l]);
		
		int ac=0;
		for(int p=a;p>0;p--) {
			for(int q=0;q<p;q++) {
				System.out.print(num2[ac]+" ");
				ac++;
			}
			System.out.println();
			for(int r=p;r>0;r--) {
				System.out.print(" ");
			}
		}
	}
}

