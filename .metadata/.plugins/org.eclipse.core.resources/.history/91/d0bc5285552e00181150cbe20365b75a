
public class LinkedList {
	
	Node head;
	
	LinkedList(){
		head=null;
	}
	
	public void insert(Node n) {
		Node t = head;
		if(t==null) {
		n.next=n;
		head=n;
		
	}	
		
		else if(t.data>=n.data) {
			while(t.next!=head) 
				t=t.next;
			t.next=n;
			n.next=head;
			head=n;
					}
		
		else  {
			while(t.next!=head && t.next.data<n.data) 
				t=t.next;
			n.next=t.next;
			t.next=n;
			
		}
		
	}
	
	public void display() {
		//if(head!=null) {
		Node t=head;
		/*do{
			
			System.out.println(t.data);
			t=t.next;
		}while(t!=head);*/
		while(t==head || t.next!=head) {
			
			System.out.println(t.data);
			t=t.next;
		}
		
		
	}
	
	public static void main(String[] args) {
		LinkedList l = new LinkedList();
		int a[]=new int[] {10,2,7,11};
		Node temp=null;
		for(int i=0;i<a.length;i++) {
			temp = new Node(a[i]);
			l.insert(temp);
		}
		
		l.display();
	}
}
