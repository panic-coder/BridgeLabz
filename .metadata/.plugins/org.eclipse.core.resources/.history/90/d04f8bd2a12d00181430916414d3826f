
public class Ascending {
	AscendingMain a[];
	int size;
	int top;
	
	public void CompareTo() {
		top=-1;
		this.size=50;
		a= new AscendingMain[this.size];
	}
	
	public void CompareTo(int i) {
		top=-1;
		this.size=i;
		a=new AscendingMain[this.size];
	}
	

	public void push(AscendingMain item) {
		if(isFull()) {
			top++;
			a[top]=item;
		}
	}
	
	public boolean isFull() {
		if(top==size)
			return false;
		else
			return true;
	}
	
	
	
}
