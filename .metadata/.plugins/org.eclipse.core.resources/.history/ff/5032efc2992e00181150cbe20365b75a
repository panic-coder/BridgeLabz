
public class AscendingMain{
	public static void main(String[] args) {
		Values c[] = { new Values("shubham","3"),
					   new Values("deep","1"),
					   new Values("raj","2"),
					   new Values("vinay","10"),
					   new Values("deepak","8")};
								
		for (int i = 0; i < c.length; i++) {
			for (int j = 0; j < c.length-i-1; j++)
				if(compare(c[j].roll,c[j+1].roll)>0)
				{
					Values temp=c[j];
					c[j]=c[j+1];
					c[j+1]=temp;
				}
		}
		System.out.println("sorted by roll\n");
		for (int i = 0; i < c.length; i++) {
			System.out.println(c[i]);
		}
	}
	public static int compare(String s1,String s2)
	{
		int len=0;
		if(s1.length()>s2.length())
		{
			len=s2.length();
		}
		else {
			len=s1.length();
			System.out.println(len);
		}
		for (int i = 0; i < len; i++) {
			while(i<len&&s1.charAt(i)-s2.charAt(i)==0)//go inside
			{
				i++;
			}
			if(s1.length()==i&&s2.length()==i)//both are equal
				return 0;
			if(s1.length()==i||s2.length()==i) {
				if(s1.length()==i) {
					return -1;//according to length return
				}
				else return 1;
			}
			return (s1.charAt(i)-s2.charAt(i));//return the value

		}
		return 0;
	}
}
