
public class StackSearch {
	int a[];
	int size;
	int top;
	
	public StackSearch() {
		top=-1;
		this.size=50;
		a= new int[this.size];
	}
	
	public StackSearch(int i) {
		top=-1;
		this.size=i;
		a=new int[this.size];
	}
	

	public void push(int item) {
		if(isFull()) {
			top++;
			a[top]=item;
		}
	}
	
	public boolean isFull() {
		if(top==size)
			return false;
		else
			return true;
	}
	
	public void pop() {
		while(top>=0) {
			System.out.println(a[top]);
			top--;
		}
	}
}
