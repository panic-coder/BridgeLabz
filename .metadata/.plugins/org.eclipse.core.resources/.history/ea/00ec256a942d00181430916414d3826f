
public class StackSearch {
	int a[];
	int size;
	int top;
	
	public StackSearch() {
		top=-1;
		this.size=50;
		a= new int[this.size];
	}
	
	public StackSearch(int i) {
		top=-1;
		this.size=i;
		a=new int[this.size];
	}
	

	public void push(int item) {
		if(isFull()) {
			top++;
			a[top]=item;
		}
	}
	
	public boolean isFull() {
		if(top==size)
			return false;
		else
			return true;
	}
	
	public void pop() {
		while(top>=0) {
			System.out.println(a[top]);
			top--;
		}
	}
	
	public void search(int searchItem) {
		top++;int c=0;
		while(top<size) {
			if(a[top]==searchItem) {
				c++;
			}
			top++;
		}
		if(c>0)
			System.out.println(searchItem+" is present");
		else
			System.out.println(searchItem+" is not present");
		
	}
	
	public void sort() {
		
	}
}
