
public class LinkedList {
	
	Node head;
	Node t;
	private Node t2;
	
	LinkedList(){
		head=null;
	}
	
	public void insert(Node n) {
		t2 = head;
		if(t2==null) {
		t2.next=n;
		head=n;
		
	}	
		
		else if(t2.data>n.data) {
			while(t2.next!=head) 
				t2=t2.next;
			t2.next=n;
			n.next=head;
			head=n;
					}
		
		else  {
			while(t2.next!=head && t2.next.data<n.data) 
				t2=t2.next;
			n.next=t2.next;
			t2.next=n;
			
		}
		
	}
	
	public void display() {
		Node t=head;
		while(t!=null) {
			
			System.out.println(t.data);
			t=t.next;
		}
	}
	
	public static void main(String[] args) {
		LinkedList l = new LinkedList();
		int a[]=new int[] {10,2,7,11};
		Node temp=null;
		for(int i=0;i<a.length;i++) {
			temp = new Node(a[i]);
			l.insert(temp);
		}
		
		l.display();
	}
}
